#include<iostream>

using namespace std;

const int N = 1e5 + 10;
int arr[N];
int index;

int main(){
    int n;
    cin >> n;
    for(int i = 0; i < n; i++){
        int x;
        cin >> x;
        while(index && arr[index] >= x) index--;
        if(index) cout << arr[index] << " ";
        else cout << "-1" << " ";
        arr[++index] = x;
    }
    
    return 0;
}
